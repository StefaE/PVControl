    {
        "id": "24c11b92f9fd074c",
        "type": "function",
        "z": "fdea4f1a813872ee",
        "name": "",
        "func": "modeid    = msg.payload.wbstatus.modeid\nstateid   = msg.payload.wbstatus.stateid\nconnected = msg.payload.wbstatus.connected\ncompleted = msg.payload.wbstatus.charge_completed\n\nset_Amp   = msg.payload.wbstatus.manualmodeamp\nreq_Amp   = msg.payload.ctrlstatus.I_charge\nact_Amp   = msg.payload.wbstatus.active_power/(3*msg.payload.pvstatus.grid_voltage)\ncos_phi   = msg.payload.wbstatus.cos_phi*10\n\nif (global.get('cnt') == 1) {\n    msg.ui_control = {\n        'ymax': msg.payload.wbstatus.I_max\n    }\n}\nif (modeid == 3) {\n    state = 'Manual / '\n    if (stateid == 17) {\n        state += 'Disabled' \n    } else if (stateid == 0) {\n        state += 'Idle'\n    } else if (stateid == 4) {\n        state += 'Waiting'\n    } else if (stateid == 5) {\n        state += 'Charging'\n    } else {\n        state += 'ERR' + stateid.toString()\n    }\n    state += \" / \" + set_Amp.toString() + \"A\"\n} else if (modeid == 1) {\n    state = 'ECO'\n} else if (modeid == 2) {\n    state = 'Quick'\n}\n\nif (connected == 0) {\n    connect = 'not connected'\n} else if (completed == 0) {\n    connect = 'connected / not completed'\n} else {\n    connect = 'completed'\n}\nmsg.payload['state']     = state\nmsg.payload['connect']   = connect\nmsg.payload['requested'] = req_Amp.toString() + \"A\"\nmsg.payload['actual']    = act_Amp.toString() + \"A\"\nmsg.payload['cos_phi']   = cos_phi\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
