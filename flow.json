    {
        "id": "5ea917eba5f1f34b",
        "type": "function",
        "z": "fdea4f1a813872ee",
        "name": "writeOutput",
        "func": "str = \"[PVControl]\\n\";\nkeys = flow.keys().sort();\nmsg.payload = {};\nfor (var i = 0; i < keys.length; i++) {\n    key = keys[i];\n    val = flow.get(key);\n    if (key.endsWith(\"_0\")) {\n        continue;\n    } else if (key.includes('SOC')) {\n        val = val/100\n    } else if (key == 'chargeStart') {           // convert time stamp to Epoch (UTC)\n        date = new Date()\n        offs = -60000*date.getTimezoneOffset()   // offset for local time zone (vs. UTC)\n\n        now  = date.getTime() + offs             // now (in local time zone)\n\n        date.setHours(0)\n        date.setMinutes(0)\n        date.setSeconds(0)\n        date.setMilliseconds(0)                  // set date to last midnight\n\n        val   = date.getTime() + offs + val      // time to start charging\n        slack = 1*60000                          // give 1min slack: if start time is closer to this, loop over to next day\n        if (val < now - slack) {\n            val = val + 86400000                 // loop over to next day\n        }\n        val = val - offs                         // convert to UTC\n    }\n    str += \"    \" + key + \" = \" + val + \"\\n\";\n}\nmsg.payload = str;\nmsg.filename = global.get(\"projectdir\") + \"/gui_config.ini\"\nflow.set(\"saved\", new Date())\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 790,
        "y": 860,
        "wires": [
            [
                "0642c142661b7155"
            ]
        ]
    },
