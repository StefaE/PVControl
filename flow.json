    {
        "id": "8e35f5a7097972fe",
        "type": "function",
        "z": "fdea4f1a813872ee",
        "name": "I_Charge",
        "func": "msg.update = 99\nif (msg.topic == \"config\") {\n    if (global.get(\"cnt\") == 1) {\n        msg.ui_control = {\n            \"min\": msg.payload.config.wbstatus.I_min,\n            \"max\": msg.payload.config.wbstatus.I_max\n        };\n\n        msg.payload[\"I_min\"] = flow.get(\"I_min_0\");\n        msg.payload[\"I_max\"] = flow.get(\"I_max_0\");\n        flow.set(\"I_min\", msg.payload.I_min)\n        flow.set(\"I_max\", msg.payload.I_max)\n        msg.update = 0\n    }\n    \n} else if (msg.topic == \"I_min\") {\n    if (flow.get(\"I_max\") < msg.payload.I_min) {\n        msg.payload.I_max = msg.payload.I_min\n        msg.update = 2\n    }\n    flow.set(\"I_min\", msg.payload.I_min)\n} else if (msg.topic == \"I_max\") {\n    if (flow.get(\"I_min\") > msg.payload.I_max) {\n        msg.payload.I_min = msg.payload.I_max\n        msg.update = 1\n    }\n    flow.set(\"I_max\", msg.payload.I_max)\n} else {\n    node.error(\"I_Charge: unknown msg.topic \" + msg.topic, msg);\n}\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 120,
        "y": 240,
        "wires": [
